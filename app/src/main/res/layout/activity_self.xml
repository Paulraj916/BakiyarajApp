<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".selfActivity">

    <Spinner
        android:id="@+id/spinner8"
        android:layout_width="125dp"
        android:layout_height="50dp"
        android:layout_marginEnd="67dp"
        android:layout_marginBottom="12dp"
        app:layout_constraintBottom_toTopOf="@+id/spinner9"
        app:layout_constraintEnd_toEndOf="parent" />

    <Spinner
        android:id="@+id/spinner10"
        android:layout_width="125dp"
        android:layout_height="60dp"
        android:layout_marginTop="111dp"
        android:layout_marginEnd="67dp"
        android:layout_marginBottom="43dp"
        app:layout_constraintBottom_toTopOf="@+id/spinner6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button5"
        android:layout_width="130dp"
        android:layout_height="70dp"
        android:layout_marginEnd="141dp"
        android:layout_marginBottom="186dp"
        android:onClick="launchend"
        android:text="Enter"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Spinner
        android:id="@+id/spinner9"
        android:layout_width="125dp"
        android:layout_height="50dp"
        android:layout_marginEnd="67dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintEnd_toEndOf="parent" />

    <Spinner
        android:id="@+id/spinner7"
        android:layout_width="125dp"
        android:layout_height="50dp"
        android:layout_marginEnd="67dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@+id/spinner8"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/height"
        android:layout_width="125dp"
        android:layout_height="50dp"
        android:layout_marginStart="66dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="10dp"
        android:background="#A1E0E8"
        android:ems="10"
        android:hint="   Height"
        android:inputType="number"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/depth"
        app:layout_constraintEnd_toStartOf="@+id/spinner7"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button4"
        android:layout_width="125dp"
        android:layout_height="50dp"
        android:layout_marginStart="66dp"
        android:layout_marginTop="111dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="42dp"
        android:onClick="spinnervalue"
        android:text="Slap"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/length"
        app:layout_constraintEnd_toStartOf="@+id/spinner10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/length"
        android:layout_width="125dp"
        android:layout_height="50dp"
        android:layout_marginStart="66dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="9dp"
        android:background="#A1E0E8"
        android:ems="10"
        android:hint="   Length"
        android:inputType="number"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/height"
        app:layout_constraintEnd_toStartOf="@+id/spinner6"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/border"
        android:layout_width="125dp"
        android:layout_height="50dp"
        android:layout_marginStart="66dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="32dp"
        android:background="#A1E0E8"
        android:ems="10"
        android:hint="   Border"
        android:inputType="number"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintEnd_toStartOf="@+id/spinner9"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/depth"
        android:layout_width="125dp"
        android:layout_height="50dp"
        android:layout_marginStart="66dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="11dp"
        android:background="#A1E0E8"
        android:ems="10"
        android:hint="   Depth"
        android:inputType="number"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/border"
        app:layout_constraintEnd_toStartOf="@+id/spinner8"
        app:layout_constraintStart_toStartOf="parent" />

    <Spinner
        android:id="@+id/spinner6"
        android:layout_width="125dp"
        android:layout_height="50dp"
        android:layout_marginEnd="67dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/spinner7"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>