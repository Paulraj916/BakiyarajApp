<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".laftActivity">

    <EditText
        android:id="@+id/doors"
        android:layout_width="153dp"
        android:layout_height="58dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="102dp"
        android:layout_marginEnd="47dp"
        android:layout_marginBottom="77dp"
        android:ems="10"
        android:hint="no of doors"
        android:inputType="number"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/inches1"
        app:layout_constraintEnd_toStartOf="@+id/button2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/totalheight"
        android:layout_width="147dp"
        android:layout_height="43dp"
        android:layout_marginStart="64dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="42dp"
        android:ems="10"
        android:hint="total height"
        android:inputType="number"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/spinner4"
        app:layout_constraintEnd_toStartOf="@+id/spinner3"
        app:layout_constraintStart_toStartOf="parent" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="110dp"
        android:layout_height="47dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="35dp"
        app:layout_constraintBottom_toTopOf="@+id/spinner2"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/inches1"
        android:layout_width="150dp"
        android:layout_height="47dp"
        android:layout_marginStart="64dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="35dp"
        android:ems="10"
        android:hint="length inches"
        android:inputType="number"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/inches2"
        app:layout_constraintEnd_toStartOf="@+id/spinner"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/inches2"
        android:layout_width="150dp"
        android:layout_height="47dp"
        android:layout_marginStart="64dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="29dp"
        android:ems="10"
        android:hint="height inches"
        android:inputType="number"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/totalheight"
        app:layout_constraintEnd_toStartOf="@+id/spinner2"
        app:layout_constraintStart_toStartOf="parent" />

    <Spinner
        android:id="@+id/spinner4"
        android:layout_width="110dp"
        android:layout_height="49dp"
        android:layout_marginStart="154dp"
        android:layout_marginEnd="147dp"
        android:layout_marginBottom="27dp"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="110dp"
        android:layout_height="48dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toTopOf="@+id/spinner3"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/button2"
        android:layout_width="65dp"
        android:layout_height="53dp"
        android:layout_marginTop="102dp"
        android:layout_marginEnd="82dp"
        android:layout_marginBottom="82dp"
        android:hint="ok"
        android:onClick="spinnervalue"
        android:text="ok"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/spinner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinner3"
        android:layout_width="110dp"
        android:layout_height="48dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="37dp"
        app:layout_constraintBottom_toTopOf="@+id/spinner4"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="130dp"
        android:layout_height="49dp"
        android:layout_marginStart="144dp"
        android:layout_marginEnd="137dp"
        android:layout_marginBottom="126dp"
        android:onClick="launchend"
        android:text="Enter"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>